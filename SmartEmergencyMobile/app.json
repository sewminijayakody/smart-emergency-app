{
  "expo": {
    "name": "SmartEmergencyMobile",
    "slug": "SmartEmergencyMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Aido needs microphone access to detect fear or danger in your voice.",
        "NSLocationWhenInUseUsageDescription": "Aido uses your location to show if you are in a safe or high-risk area and to share your position during emergencies.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Aido uses your location to keep you safe even when the app is running in the background during emergencies."
      }
    },
    "android": {
      "minSdkVersion": 24,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCq20URgNLHDZQE3NHZebwW2QxnnNf867g"
        }
      },
      "package": "com.anonymous.SmartEmergencyMobile",
      "googleServicesFile": "./android/app/google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-dev-client",
      "expo-av",
      "expo-asset",
      "expo-notifications",
    "expo-location"
    ],
    "extra": {
      "eas": {
        "projectId": "31fee4fc-015b-472b-88aa-86798be4e3de"
      }
    },
    "owner": "sew12"
  }
}
